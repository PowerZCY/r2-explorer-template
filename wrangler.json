{
  "compatibility_date": "2025-04-01",
  "main": "src/index.ts",
  "name": "r2-explorer-template",
  "assets": {
    "directory": "node_modules/r2-explorer/dashboard",
    "binding": "ASSETS",
    "html_handling": "auto-trailing-slash"
  },
  "r2_buckets": [
    {
      "binding": "bucket",
      "bucket_name": "r2-explorer-bucket",
      "preview_bucket_name": "r2-explorer-bucket"
    },
    {
      "binding": "bucket_newspaper", 
      "bucket_name": "newspaper-assets",
      "preview_bucket_name": "newspaper-assets"
    },
    {
      "binding": "bucket_aspect",
      "bucket_name": "aspect-assets",
      "preview_bucket_name": "aspect-assets"
    }
  ],
  "observability": {
    "enabled": true
  },
  "upload_source_maps": true
}
